     17          //读取MQ-2 AD转换后的数字量
     18          uint ReadGasData( void )
     19          {
     20            uint reading = 0;
     21            
     22            /* Enable channel */
     23            ADCCFG |= 0x40;  
     24            
     25           
     26            /* writing to this register starts the extra conversion */
     27            ADCCON3 = 0x85;       // AVDD5 引脚  00： 64 抽取率(7 位ENOB)  0101： AIN5
     28          
     29          
     30            /* Wait for the conversion to be done */
     31            while (!(ADCCON1 & 0x80));
     32            
     33            /* Disable channel after done conversion */
     34            ADCCFG &= (0x40 ^ 0xFF); //按位异或。如1010^1111=0101（二进制）
     35            
     36            /* Read the result */
     37            reading = ADCL;
     38            reading |= (uint) (ADCH << 8); 
     39            
     40            reading >>= 8;
     41            
     42            return (reading);
     43          }
